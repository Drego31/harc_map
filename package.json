{
  "name": "harcmap",
  "version": "1.0.0",
  "description": "HarcMap",
  "main": "src/index.js",
  "scripts": {
    "clone-fonts": "node bin/clone-material-icons.js",
    "build:dev": "npm run clone-fonts && npx webpack --config webpack.dev.js --watch",
    "build:prod": "npm run clone-fonts && npx webpack --config webpack.prod.js",
    "run:dev": "NODE_ENV=development node app.js",
    "run:prod": "NODE_ENV=production node app.js",
    "run:mongodb": "docker run -p 27017:27017 -v `pwd`/config/mongodb:/docker-entrypoint-initdb.d mongo",
    "start": "npm run run:dev & npm run build:dev",
    "fill-database": "NODE_ENV=development node bin/prepareEvent.js",
    "tests:restapi": "jest --config=jest-node.config.js --testPathPattern=restapi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drego31/harc_map.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Drego31/harc_map/issues"
  },
  "homepage": "https://github.com/Drego31/harc_map#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "mongodb": "^3.5.3",
    "ncp": "^2.0.0",
    "ol": "^6.2.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sha3": "^2.1.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@dbetka/utils": "^0.1.0",
    "material-design-icons": "^3.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "import-glob-loader": "^1.1.0",
    "jest": "^25.2.7",
    "moment": "^2.24.0",
    "nodemailer": "^6.4.6",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^4.0.0",
    "vee-validate": "^3.2.5",
    "vue-loader": "^15.9.1",
    "vue-material-design-icons": "^4.4.0",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
